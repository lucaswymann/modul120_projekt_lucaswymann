<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor" A="255" R="59" G="99" B="139"></Color>
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}"></SolidColorBrush>

    <Color x:Key="SuccessColor"  A="255" R="65" G="139" B="59"></Color>
    <Color x:Key="WarningColor"  A="255" R="165" G="167" B="56"></Color>
    <Color x:Key="LinkColor"  A="255" R="56" G="114" B="167"></Color>
    <SolidColorBrush x:Key="LinkBrush" Color="{StaticResource LinkColor}"></SolidColorBrush>
    <Color x:Key="LinkColorHover" A="255" R="17" G="96" B="168"></Color>
    <SolidColorBrush x:Key="LinkHoverBrush" Color="{StaticResource LinkColorHover}"></SolidColorBrush>

</ResourceDictionary>